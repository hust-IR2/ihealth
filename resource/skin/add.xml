                    <Edit name="add_patient_id" bordersize="1" bordercolor="#FFE7E7E7" align="center" font="2" text="001" width="310" nativebkcolor="#FFe4e3e8" bkcolor="#FFe4e3e8" textcolor="#FF767676" readonly="true"/>
                    <Label width="171" text="创建时间" align="right" textcolor="#FFB7BBBF" font="2"/>
