                    <Edit name="add_patient_createtime" bordersize="1" bordercolor="#FFE7E7E7" align="center" font="2" text="" width="310" nativebkcolor="#FFe4e3e8" bkcolor="#FFe4e3e8" textcolor="#FF767676" readonly="true"/>
                    
                    <Control />
                </HorizontalLayout>
                <Control height="32"/>
                <HorizontalLayout height="41">
                    <Label width="209" text="姓名" align="right" textcolor="#FFB7BBBF" font="2"/>
