                    <Edit name="add_patient_name" bordersize="1" bordercolor="#FFE7E7E7" align="center" font="2" text="001" width="310" nativebkcolor="#FFFFFFFF" bkcolor="#FFFFFFFF" textcolor="#FF767676" readonly="false"/>
                    <Label width="171" text="上次治疗时间" align="right" textcolor="#FFB7BBBF" font="2"/>
